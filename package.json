{
  "name": "@mitakohir/nestjs-telegraf",
  "version": "2.7.3",
  "description": "Telegraf module for NestJS",
  "keywords": [
    "nest",
    "nestjs",
    "nestjs-module",
    "telegraf",
    "telegram",
    "telegram bot",
    "telegram bot api",
    "bot",
    "bot api",
    "bot framework"
  ],
  "homepage": "https://nestjs-telegraf.vercel.app",
  "bugs": {
    "url": "https://github.com/evilsprut/nestjs-telegraf/issues"
  },
  "repository": "https://github.com/MitakoHir/nestjs-telegraf",
  "license": "MIT",
  "author": "Iurii Topychkanov <iurii.topychkanov@gmail.com>",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "",
    "typedoc:build": "typedoc --tsconfig ./tsconfig.typedoc.json",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-angular": "19.8.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@types/lodash": "4.17.20",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "reflect-metadata": "0.2.2",
    "telegraf": "^4.16.3",
    "typedoc": "0.28.10",
    "typescript": "5.9.2",
    "vitepress": "^1.6.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "reflect-metadata": "^0.2.2",
    "telegraf": "^4.0.0",
    "typescript": "^4.1.2 || ^5.0.0"
  }
}
